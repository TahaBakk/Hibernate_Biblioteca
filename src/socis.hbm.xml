<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="socis" table="socis">
        <meta attribute="class-description">
            This class contains the llibres detail.
        </meta><id name="id" type="int" column="id">
        <generator />
    </id>
        <property name="nom" column="nom" type="string"/>
        <property name="cognom" column="cognom" type="int"/>
        <property name="editorial" column="editorial" type="String"/>
        <property name="nombrePagines" column="nombre_pagines" type="int"/>
        <property name="anydedicio" column="any_dedicio" type="int"/>
    </class>
</hibernate-mapping>